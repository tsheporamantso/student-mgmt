<h1>Students List</h1>

<table>
<thead>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email address</th>
  </tr>
</thead>
<tbody>
  <% @students.each do |student| %>
    <tr>
      <th><%= student.first_name %></th>
      <th><%= student.last_name %></th>
      <th><%= student.email %></th>
    </tr>
  <% end %>
</tbody>
</table>